[user]
  name = gbertl
  email = gilbertlcsndle@gmail.com
[core]
  editor = nvim
  fileMode = false
  excludesFile = ~/.gitignore
[alias]
  l = log --oneline
  dc = diff --cached
  sv = !git add -A && git commit -m 'wip'
  can = !git add -A && git commit --amend --no-edit
[init]
  defaultBranch = main
[safe]
  directory = *
